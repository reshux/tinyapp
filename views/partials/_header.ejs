      <td>
        <form class action="/logout" method="POST"><input type="submit" class="btn btn-primary" value="Logout"></form>
     </td>
    </tr>
  </table>
  <% } else { %>
  <table style="width: 300px">
  <%= templateVars.username %>
    <tr>
      <td>
        <a href="/login">Login</a>
      </td>
      <td>
        <a href="/register">Register</a>
     </td>
     <td>
    </tr>
  </table>
