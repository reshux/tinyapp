<header> <h1> TinyApp </h1> </header>
<% if (templateVars.username) { %>
  <table style="width: 250px">
    <tr>
      <td> Logged in as <%= templateVars.username %> </td>
      <td>
        <form class action="/logout" method="POST"><input type="submit" class="btn btn-primary" value="Logout"></form>
     </td>
    </tr>
  </table>
  <% } else { %>
  <table style="width: 300px">
  <%= templateVars.username %>
    <tr>
      <td>
        <form action="/login" method="POST"><input class="form-control" name="username" type="text" placeholder="username" />
      </td>
      <td>
        <form class action="/login" method="POST"><input type="submit" class="btn btn-primary" value="Login"></form>
     </td>
    </tr>
  </table>
   <% } %>
